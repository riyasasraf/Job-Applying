{
  "manifest_version": 3,
  "name": "LinkedIn Job Extractor",
  "version": "1.0",
  "description": "Extracts job details from LinkedIn job listings.",
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icons/icon48.png"
    }
  },
  "permissions": ["activeTab", "scripting"],
  "host_permissions": [
    "*://*.linkedin.com/*",
    "http://127.0.0.1:5000/*" 
  ]
}
